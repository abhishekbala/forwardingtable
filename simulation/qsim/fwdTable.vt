module camTestBench_vlg_sample_tst(
	Clock,
	Reset,
	Trigger,
	sampler_tx
);
input  Clock;
input  Reset;
input  Trigger;
output sampler_tx;

reg sample;
time current_time;
always @(Clock or Reset or Trigger)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule


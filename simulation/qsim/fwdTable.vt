module camTestBench_vlg_vec_tst();
// constants                                           
// general purpose registers
reg Clock;
reg Reset;
reg Trigger;
// wires                                               
wire count_enable_d;
wire [11:0] count_val_d;
wire [2:0] data_out;
wire read_state;
wire start_out;
wire test_good;

wire sampler;                             

// assign statements (if any)                          
camTestBench i1 (
// port map - connection between master ports and signals/registers   
	.Clock(Clock),
	.count_enable_d(count_enable_d),
	.count_val_d(count_val_d),
	.data_out(data_out),
	.read_state(read_state),
	.Reset(Reset),
	.start_out(start_out),
	.test_good(test_good),
	.Trigger(Trigger)
);

// Clock
always
begin
	Clock = 1'b0;
	Clock = #10000 1'b1;
	#10000;
end 

// Reset
initial
begin
	Reset = 1'b1;
	Reset = #20000 1'b0;
end 

// Trigger
initial
begin
	Trigger = 1'b0;
	Trigger = #140000 1'b1;
	Trigger = #20000 1'b0;
end 

camTestBench_vlg_sample_tst tb_sample (
	.Clock(Clock),
	.Reset(Reset),
	.Trigger(Trigger),
	.sampler_tx(sampler)
);

camTestBench_vlg_check_tst tb_out(
	.count_enable_d(count_enable_d),
	.count_val_d(count_val_d),
	.data_out(data_out),
	.read_state(read_state),
	.start_out(start_out),
	.test_good(test_good),
	.sampler_rx(sampler)
);
endmodule

